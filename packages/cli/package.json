{
  "name": "@kitajs/cli",
  "version": "1.1.0",
  "description": "Performant and type safe fastify router - Build fast end-to-end APIs with ZERO abtraction cost!",
  "funding": "https://github.com/sponsors/arthurfiorette",
  "license": "MIT",
  "author": "Arthur Fiorette <kita@arthur.place>",
  "main": "dist/index.js",
  "bin": {
    "kita": "bin/run",
    "kita-dev": "bin/dev",
    "kitajs": "bin/run",
    "kitajs-dev": "bin/dev"
  },
  "files": [
    "bin",
    "dist",
    "npm-shrinkwrap.json",
    "oclif.manifest.json"
  ],
  "scripts": {
    "build": "tsc",
    "postbuild": "oclif manifest && oclif readme",
    "start": "node -r tsx/csj index.ts"
  },
  "dependencies": {
    "@kitajs/common": "*",
    "@kitajs/generator": "*",
    "@kitajs/parser": "*",
    "@oclif/core": "^3.0.3",
    "@oclif/plugin-autocomplete": "^2.3.9",
    "@oclif/plugin-help": "^5.2.20",
    "chalk": "^4.1.2",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@types/node": "^20.8.3",
    "oclif": "^4.0.0",
    "tsx": "^3.13.0"
  },
  "peerDependencies": {
    "@kitajs/runtime": ">=1",
    "typescript": "^5"
  },
  "oclif": {
    "bin": "kita",
    "dirname": "kita",
    "commands": "dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete"
    ],
    "hooks": {
      "init": "dist/hooks/init"
    }
  }
}
